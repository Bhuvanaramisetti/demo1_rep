pipeline
{
    agent any
    stage
    {
        stage("clone")
        {
            steps{
                git "https://github.com/Bhuvanaramisetti/demo1_rep.git"
            }
        }
        stage("build")
        {
            steps{
sh "javac hello.java"
}
            }
        stage("run")
        {
            steps
            {
                sh'javac java.hello'
            }
        }
    }
}
